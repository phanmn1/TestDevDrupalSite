{
  "name": "drupal/webprofiler",
  "description": "Drupal Web Profiler.",
  "type": "drupal-module",
  "authors": [
    {
      "name": "Luca Lusso",
      "email": "lussoluca@gmail.com",
      "homepage": "https://github.com/lussoluca",
      "role": "Maintainer"
    }
  ],
  "license": "GPL-2.0+",
  "require": {
    "composer/installers": "^1.3",
    "d3/d3": "3.5.*",
    "components/highlightjs": "9.7.*"
  },
  "installer-paths": {
	"libraries/{$name}": ["type:drupal-library"]		
  },
  "repositories": {
		"d3": {
			"type": "package",
			"package": {
				"name": "d3/d3",
				"version": "v3.5.17",
				"type": "drupal-library",
				"dist": {
					"url": "https://github.com/d3/d3/archive/v3.5.17.zip",
					"type": "zip"
				}
			}
		},
		"highlightjs": {
			"type": "package",
			"package": {
				"name": "components/highlightjs",
				"version": "9.7.0",
				"type": "drupal-library",
				"dist": {
					"url": "https://github.com/components/highlightjs/archive/9.7.0.zip",
					"type": "zip"
				}
			}
		}
	}
  
}
